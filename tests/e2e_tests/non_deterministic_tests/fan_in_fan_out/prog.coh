actor Worker {
    coord: Main;
    new create(Main c) {
        coord := c;
    }

    be compute(int v) {
        coord->receive_confirmation(v);
    }
}

actor Main {
    new create() {
        var worker_arr: Worker ref = new[10000] Worker(new Worker.create(this));
        var i: int = 0;
        while(i < 10000) {
            worker_arr[i] = new Worker.create(this);
            worker_arr[i]->compute(i);
            i = i + 1;
        }
    }
    be receive_confirmation(int v) {
        OUT v;
    }
}