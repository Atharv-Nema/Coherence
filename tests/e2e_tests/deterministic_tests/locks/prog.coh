
actor Other {
    a: int locked<A>;
    new create((int locked<A>) lock_int) {
        a := lock_int;
    }
    be increment() {
        atomic {
            OUT a[0];
            a[0] = a[0] + 1;
        }
    }
}

actor Main {
    new create() {
        var lock_var: int locked<A> = new[1] int(0);
        var other_arr: Other ref = new[10] Other(new Other.create(lock_var));
        var ind: int = 1;
        while(ind < 10) {
            other_arr[ind] = new Other.create(lock_var);
            ind = ind + 1;
        }
        ind = 0;
        while(ind < 10) {
            other_arr[ind]->increment();
            ind = ind + 1;
        }
    }
}