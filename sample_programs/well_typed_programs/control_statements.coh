func random_func(int a_) => int {
    if(a_ == 1) {
        return 1;
    }
    else {
        a_ = 3;
        return 2;
    }
    while(a_ > 0) {
        a_ = a_ - 1;
        return 1;
    }
    return 0;
}

actor Main {
    new create() {
        var a: int = 1;
        if(a == 1) {
            a = a + 1;
        }
        else {
            a = a + 2;
        }
        while(a > 0) {
            a = a - 1;
        }
    }
}